{
  "name": "sclip-s-automation-service",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Sclipper/sclip-s-automation-service.git",
  "author": "Sclipper",
  "license": "MIT",
  "scripts": {
    "start:dev": "NODE_ENV=dev NODE_PATH=src nodemon index.ts",
    "start:prod": "NODE_ENV=prod NODE_PATH=src nodemon index.ts",
    "migrate:dev": "npx dotenv -e .env.dev -- prisma migrate dev",
    "db:push": "npx dotenv -e .env.dev -- prisma db push",
    "migrate:reset": "npx dotenv -e .env.dev -- prisma migrate reset",
    "db:seed": "npx dotenv -e .env.dev -- prisma db seed",
    "prisma:generate": "npx dotenv -e .env.dev -- prisma generate",
    "prisma:studio": "npx dotenv -e .env.dev -- prisma studio",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-typescript": "^7.17.12",
    "@prisma/client": "5.2.0",
    "@sentry/node": "^7.8.1",
    "@sentry/tracing": "^7.8.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/nodemailer": "^6.4.9",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "global": "^4.4.0",
    "jsonschema": "^1.4.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.3",
    "nodemailer": "^6.9.5",
    "nodemon": "^2.0.16",
    "prisma": "^5.2.0",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-jest": "^28.1.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "typescript": "^4.7.4"
  }
}
